exportar funcion contador(elemento) {
    mut conteo = 0

    const establecemos_el_conteo = (propiedades) => {
        const valor = Object.values(propiedades)

        const nuevo_valor = valor[0]

        conteo = nuevo_valor
        elemento.innerHTML = `Contador en ${nuevo_valor}`
    }

    elemento.addEventListener('click', () => establecemos_el_conteo({
        a: conteo + 1
    }))

    establecemos_el_conteo({
        a:  0
    })
}
